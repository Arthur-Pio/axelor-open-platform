{
  "palette": {
    "blue": "#6C67FA",
    "green": "#3ECF8E",
    "purple": "#C354F2",
    "red": "#DD514C",
    "yellow": "#FFBE4E",
    "cyan": "#26C6F9",

    "white": "#FFF",
    "black": "#333",

    "body_color": "#32325D",
    "emphasis_color": "#33325B"
  },
  "border": {
    "color": "#EAEAFE"
  },
  "typography": {
    "body": {
      "fontSize": "15px"
    }
  },
  "components": {
    "Shell": {
      "sidebar": {
        "bg": "#F8F8FF"
      }
    },
    "Panel": {
      "header": {
        "bg": "#FBFBFF"
      },
      "title": {
        "padding": "0.25rem 0.5rem"
      }
    },
    "Dropdown": {
      "border": {
        "color": "#EAEAFE"
      },
      "item_hover": {
        "bg": "#F8F8FF"
      },
      "item_active": {
        "bg": "primary",
        "color": "#fff"
      },
      "divider": {
        "bg": "#EAEAFE"
      }
    },
    "NavMenu": {
      "item": {
        "border": {
          "radius": "1rem"
        }
      },
      "item_hover": {
        "bg": "var(--bs-primary-bg-subtle)"
      },
      "item_active": {
        "bg": "rgba(var(--bs-primary-rgb), 0.8)",
        "color": "white"
      },
      "icon": {
        "bg": "transparent"
      },
      "icon_hover": {
        "bg": "transparent"
      },
      "icon_active": {
        "color": "white"
      }
    },
    "NavTabs": {
      "item": {
        "padding": "0.25rem 0.5rem"
      },
      "text": {
        "transform": "none"
      },
      "icon": {
        "bg": "transparent"
      },
      "icon_hover": {
        "bg": "transparent"
      },
      "icon_active": {
        "bg": "transparent"
      }
    },
    "CommandBar": {
      "button": {
        "padding": "0.25rem"
      },
      "button_hover": {
        "bg": "#F8F8FF"
      },
      "button_active": {
        "bg": "#EAEAFE"
      }
    },
    "Table": {
      "bg": "#FFF",
      "border": {
        "color": "#EFEDFE"
      },
      "header": {
        "bg": "#FFF"
      },
      "row_odd": {
        "bg": "#FBFBFF"
      },
      "row_hover": {
        "bg": "#F0F0FD"
      },
      "row_active": {
        "bg": "#F0F0FD"
      },
      "cell_active": {
        "bg": "#E8E8FD"
      },
      "cell": {
        "padding": "0.375rem 0.5rem"
      }
    },
    "Button": {
      "paddingX": "0.5rem",
      "paddingY": "0.25rem"
    },
    "Badge": {
      "padding": "0.25rem 0.5rem",
      "border": {
        "radius": "1rem"
      }
    },
    "Input": {
      "padding": "0.25rem 0.5rem",
      "focus": {
        "shadow": "none"
      },
      "invalid_focus": {
        "shadow": "none"
      },
      "placeholder": {
        "color": "var(--bs-tertiary-color)"
      }
    }
  }
}
