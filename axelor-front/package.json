{
  "name": "axelor-front",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --config vite-dev.config.ts",
    "clean": "rimraf dist",
    "build": "tsc && vite build",
    "build:staging": "tsc && vite build --config vite-staging.config.ts",
    "preview": "vite preview",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,css,scss,html}\"",
    "test": "vitest",
    "lint": "eslint src"
  },
  "dependencies": {
    "@axelor/ui": "^0.0.13",
    "@babel/standalone": "^7.21.2",
    "bootstrap-icons": "^1.10.3",
    "bootstrap-sass": "2.3.2",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "dayjs": "^1.11.7",
    "dompurify": "^3.0.1",
    "echarts": "^5.4.2",
    "font-awesome": "^4.7.0",
    "immer": "^9.0.19",
    "jotai": "^2.0.2",
    "jotai-immer": "^0.2.0",
    "jotai-molecules": "^1.1.0",
    "jotai-optics": "^0.3.0",
    "jsdom": "^21.1.0",
    "lodash": "^4.17.21",
    "monaco-editor": "^0.36.1",
    "optics-ts": "^2.4.0",
    "parse5": "^7.1.2",
    "react": "^18.2.0",
    "react-datepicker": "^4.11.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.2",
    "react-text-mask": "^5.5.0",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/dompurify": "^3.0.0",
    "@types/echarts": "^4.9.16",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.2",
    "@types/react": "^18.0.28",
    "@types/react-datepicker": "^4.10.0",
    "@types/react-dom": "^18.0.11",
    "@types/react-text-mask": "^5.4.11",
    "@types/react-transition-group": "^4.4.5",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-app": "^7.0.1",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "sass": "^1.58.3",
    "typescript": "^5.0.2",
    "vite": "^4.2.0",
    "vite-plugin-svgr": "^2.4.0",
    "vitest": "^0.29.3"
  },
  "packageManager": "pnpm@8.1.0",
  "pnpm": {
    "patchedDependencies": {
      "bootstrap-sass@2.3.2": "patches/bootstrap-sass@2.3.2.patch",
      "font-awesome@4.7.0": "patches/font-awesome@4.7.0.patch"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/*",
        "@eslint/*"
      ]
    }
